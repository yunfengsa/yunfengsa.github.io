{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      },
      "useBuiltIns": "usage"
    }], "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties", ["import", {
    "libraryName": "@material-ui/core",
    "libraryDirectory": "components",  // default: lib
    "camel2DashComponentName": false,  // default: true
  }]]
}